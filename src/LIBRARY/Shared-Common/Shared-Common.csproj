		<IncludeAssets>runtime; compile; build; native; contentfiles; analyzers</IncludeAssets>
	</PackageReference>
  </ItemGroup>	

  <Import Project="..\Shared-Common-Base\Shared-Common-Base.projitems" Label="Shared" />
	
