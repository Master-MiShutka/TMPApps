<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Controls="clr-namespace:TMP.Wpf.CommonControls">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="pack://application:,,,/TMP.Wpf.CommonControls;component/Styles/Clean/CleanGroupBox.xaml" />
        <ResourceDictionary Source="pack://application:,,,/TMP.Wpf.CommonControls;component/Styles/Clean/CleanWindow.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style BasedOn="{StaticResource CleanGroupBoxStyleKey}" TargetType="GroupBox" />
    <Style BasedOn="{StaticResource CleanWindowStyleKey}" TargetType="{x:Type Controls:TMPWindow}" />
</ResourceDictionary>