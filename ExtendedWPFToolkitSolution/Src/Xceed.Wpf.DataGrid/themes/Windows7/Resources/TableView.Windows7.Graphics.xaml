<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Xceed.Wpf.DataGrid"
    xmlns:markup="clr-namespace:Xceed.Wpf.DataGrid.Markup">

    <ResourceDictionary.MergedDictionaries>

        <!--  Include the standard graphics  -->
        <markup:DataGridThemeResourceDictionary Source="../../Common/Graphics.xaml" />

        <!--  Needs Windows7 Resources  -->
        <markup:DataGridThemeResourceDictionary Source="Windows7.Resources.xaml" />

    </ResourceDictionary.MergedDictionaries>

    <!--  Glyph that is displayed in the group headers to expand the group.  -->
    <DataTemplate x:Key="expandGroupGlyph">
        <Grid>
            <Border
                MinWidth="24"
                MinHeight="24"
                Background="Transparent"
                SnapsToDevicePixels="True">
                <Border
                    MinWidth="16"
                    MinHeight="16"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    BorderBrush="{StaticResource basicBlackBorderBrush}"
                    BorderThickness="0,0,1,1"
                    CornerRadius="4"
                    SnapsToDevicePixels="True">
                    <Border
                        BorderBrush="{StaticResource basicWhiteBorderBrush}"
                        BorderThickness="0,0,1,1"
                        CornerRadius="3"
                        SnapsToDevicePixels="True" />
                </Border>
            </Border>

            <Path
                Width="10"
                Height="2"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Data="M 6.5,4.5 C6.5,4.5 2.5,4.5 2.5,4.5 "
                Opacity="1"
                SnapsToDevicePixels="True"
                Stretch="Fill"
                Stroke="#888"
                StrokeThickness="2" />
            <Path
                Width="2"
                Height="10"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Data="M 4,6 C4,6 4,2 4,2 "
                Opacity="1"
                SnapsToDevicePixels="True"
                Stretch="Fill"
                Stroke="#888"
                StrokeThickness="2" />
        </Grid>
    </DataTemplate>

    <!--  Glyph that is displayed in the group headers to collapse the group.  -->
    <DataTemplate x:Key="collapseGroupGlyph">
        <Grid>
            <Border
                MinWidth="24"
                MinHeight="24"
                Background="Transparent"
                SnapsToDevicePixels="True">
                <Border
                    MinWidth="16"
                    MinHeight="16"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    BorderBrush="{StaticResource basicBlackBorderBrush}"
                    BorderThickness="0,0,1,1"
                    CornerRadius="4"
                    SnapsToDevicePixels="True">
                    <Border
                        BorderBrush="{StaticResource basicWhiteBorderBrush}"
                        BorderThickness="0,0,1,1"
                        CornerRadius="3"
                        SnapsToDevicePixels="True" />
                </Border>
            </Border>

            <Path
                Width="10"
                Height="2"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Data="M 6.5,4.5 C6.5,4.5 2.5,4.5 2.5,4.5 "
                Opacity="1"
                SnapsToDevicePixels="True"
                Stretch="Fill"
                Stroke="#888"
                StrokeThickness="2" />
        </Grid>
    </DataTemplate>

