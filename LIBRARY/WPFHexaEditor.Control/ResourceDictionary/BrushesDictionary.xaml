                    xmlns:local="clr-namespace:WPFHexaEditor.Control.ResourceDictionary">

    <SolidColorBrush x:Key="FirstColor" Color="#CC0014FF"/>
    <SolidColorBrush x:Key="SecondColor" Color="#CC0078FF"/>
