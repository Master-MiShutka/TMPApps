<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:TMP.ARMTES.Controls"
    xmlns:conv="clr-namespace:TMP.ARMTES.Converters"
    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <LinearGradientBrush x:Key="HeaderGradientBackground" StartPoint="0,0" EndPoint="0,1">
        <LinearGradientBrush.GradientStops>
            <GradientStop Offset="0.15" Color="#F6F6F6" />
            <GradientStop Offset="0.45" Color="#EAE8E8" />
            <GradientStop Offset="0.7" Color="#DCD9D9" />
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ListGridLinesBrush" Color="{DynamicResource AccentColor3}" />

    <SolidColorBrush x:Key="CollectorListBackground" Color="Transparent" />

    <sys:Double x:Key="imageWidthAndHeight">16</sys:Double>


</ResourceDictionary>